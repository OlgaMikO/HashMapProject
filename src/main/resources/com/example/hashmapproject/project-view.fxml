<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>


<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ListView?>


<VBox alignment="CENTER"
      spacing="10.0"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.hashmapproject.Controller"
      fx:id="controller"
      prefWidth="1000"
      prefHeight="500"
      onKeyPressed="#key"
>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>

    <Button
            text="Save"
            onMouseClicked="#onClick"
    >
    </Button>

    <FlowPane
            hgap="5"
            alignment="CENTER"
    >
        <TextField
                fx:id="field1"
                prefWidth="100"
        >
        </TextField>

        <TextField
                fx:id="field2"
                prefWidth="100"
        >
        </TextField>

        <TextField
                fx:id="field3"
                prefWidth="100"
        >
        </TextField>

        <TextField
                fx:id="field4"
                prefWidth="100"
        >
        </TextField>

        <TextField
                fx:id="field5"
                prefWidth="100"
        >
        </TextField>

        <TextField
                fx:id="field6"
                prefWidth="100"
        >
        </TextField>

        <TextField
                fx:id="field7"
                prefWidth="100"
        >
        </TextField>

        <TextField
                fx:id="field8"
                prefWidth="100"
        >
        </TextField>

    </FlowPane>

    <FlowPane
            hgap="5"
            alignment="CENTER"
    >

        <ListView
                fx:id="listView1"
                prefWidth="100"
        >
        </ListView>

        <ListView
                fx:id="listView2"
                prefWidth="100"
        >
        </ListView>

        <ListView
                fx:id="listView3"
                prefWidth="100"
        >
        </ListView>

        <ListView
                fx:id="listView4"
                prefWidth="100"
        >
        </ListView>

        <ListView
                fx:id="listView5"
                prefWidth="100"
        >
        </ListView>

        <ListView
                fx:id="listView6"
                prefWidth="100"
        >
        </ListView>

        <ListView
                fx:id="listView7"
                prefWidth="100"
        >
        </ListView>

        <ListView
                fx:id="listView8"
                prefWidth="100"
        >
        </ListView>


    </FlowPane>


</VBox>
